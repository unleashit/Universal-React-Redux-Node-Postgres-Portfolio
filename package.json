{
  "name": "jg",
  "version": "1.6.1",
  "description": "Portfolio of JasonGallagher",
  "main": "server/app.js",
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "local-prefix": "docker-compose -f docker-compose.yml -f docker-compose-prod.yml",
    "registry-prefix": "docker-compose -f docker-compose.yml -f docker-compose-prod.yml -f docker-compose-registry.yml",
    "dev": "docker-compose up",
    "dev:detached": "docker-compose up -d",
    "prod": "npm run local-prefix -- up -d",
    "prod:registry": "npm run registry-prefix -- up -d",
    "stop": "docker-compose stop",
    "stop:prod": "npm run local-prefix -- stop",
    "stop:registry": "npm run registry-prefix -- stop",
    "down": "docker-compose down",
    "down:prod": "npm run local-prefix -- down",
    "down:registry": "npm run registry-prefix -- down",
    "logs": "docker-compose logs -f",
    "logs:prod": "npm run local-prefix -- logs -f",
    "logs:registry": "npm run registry-prefix -- logs -f",
    "build": "docker exec -it jasongallagher_app-dev npm run build:script",
    "build:script": "rm -rf dist && NODE_ENV=production webpack --mode production --config ./webpack.production.config.js",
    "build:docker": "npm run build:docker:prod && npm run build:docker:db",
    "build:docker:prod": "docker build -t jasongallagher_app:prod -f Dockerfile-app . && docker build --target build -t jasongallagher_app:dev -f Dockerfile-app .",
    "build:docker:staging": "docker build -t jasongallagher_app:staging -f Dockerfile-app . && docker build --target build -t jasongallagher_app:dev -f Dockerfile-app .",
    "build:docker:db": "docker build -t jasongallagher_db:prod -f ./database/Dockerfile-db ./database && docker build --target staging -t jasongallagher_db:staging -f ./database/Dockerfile-db ./database",
    "attach": "docker exec -it jasongallagher_app-dev /bin/bash",
    "attach:staging": "docker exec -it jasongallagher_app-staging /bin/sh",
    "attach:prod": "docker exec -it jasongallagher_app-prod /bin/sh",
    "attach:db": "docker exec -it jasongallagher_db-staging bash -c 'psql -U ${POSTGRES_USER} -d ${POSTGRES_DB}'",
    "attach:prod:db": "docker exec -it jasongallagher_db-prod bash -c 'psql -U ${POSTGRES_USER} -d ${POSTGRES_DB}'",
    "test": "docker exec -it -e NODE_ENV=test jasongallagher_app-dev jest",
    "test:watch": "docker exec -it -e NODE_ENV=test jasongallagher_app-dev jest --watch",
    "test:coverage": "docker exec -it -e NODE_ENV=test jasongallagher_app-dev jest --coverage",
    "test:e2e": "docker exec -it -e NODE_ENV=test cypress open",
    "seed": "docker exec -it jasongallagher_app-dev node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "docker exec -it jasongallagher_app-dev node_modules/.bin/sequelize db:seed:undo:all",
    "stats": "npm run stats:build && npm run stats:display",
    "stats:build": "docker exec -it jasongallagher_app-dev bash -c 'webpack --config ./webpack.production.config.js --profile --json > stats.json'",
    "stats:display": "docker exec -it jasongallagher_app-dev webpack-bundle-analyzer stats.json dist -h 0.0.0.0"
  },
  "author": "Jason Gallagher",
  "license": "RESTRICTED",
  "browserslist": [
    "defaults",
    "> 0.1%"
  ],
  "dependencies": {
    "animate.css": "^4.1.0",
    "@babel/register": "^7.10.5",
    "@babel/core": "^7.11.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/cli": "^7.10.5",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-function-bind": "^7.10.5",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "core-js": "^3.6.5",
    "babel-plugin-react-transform": "^3.0.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-pg-simple": "^6.2.0",
    "cookie-parser": "^1.4.5",
    "crypto": "^1.0.1",
    "ejs": "^3.1.3",
    "express": "^4.17.1",
    "express-redirect": "^1.2.2",
    "express-session": "^1.17.1",
    "font-awesome": "^4.7.0",
    "history": "^5.0.0",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-loader": "^2.1.2",
    "lodash": "^4.17.20",
    "moment": "^2.29.4",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.11",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport.socketio": "^3.7.0",
    "pg": "^8.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.1",
    "react-router": "^3.2.6",
    "redux": "^4.0.5",
    "redux-form": "^8.3.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "smoothscroll-polyfill": "^0.4.4",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "css-loader": "2.1.1",
    "cssnano": "^4.1.10",
    "debug": "^4.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.7.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.6",
    "express-debug": "^1.1.1",
    "fetch-mock": "^9.10.6",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^4.3.0",
    "ignore-styles": "^5.0.1",
    "image-webpack-loader": "6.0.0",
    "jest": "^26.4.0",
    "loader-utils": "^2.0.0",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "purifycss-webpack-plugin": "^2.0.3",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.12.21",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "^1.5.4",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "jest": {
    "cacheDirectory": "<rootDir>/jestCache",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "<rootDir>/coverage",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "app/js/**/*.{js,jsx}"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/(config|database|node_modules|server|public)/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
